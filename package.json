    "bootstrap": "^4.3.1",
    "cross-env": "^6.0.0",
    "debug": "^4.1.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "googleapis": "^43.0.0",
    "memory-cache": "^0.2.0",
    "morgan": "^1.9.1",

{
  "engines": {
    "node": ">=10.5"
  },
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "cross-env DEBUG=mailcheck-* npm run build && npm run server",
    "client": "DEBUG=mailcheck-* webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "DEBUG=mailcheck-* nodemon --exec babel-node src/server/index.js",
    "dev": "cross-env DEBUG=mailcheck-* concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
